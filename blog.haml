#blog
  %h1.pagination-centered Blog Posts

  %hr

  %ul.posts
    {% for post in site.posts %}
    %li
      %span 
        {{ post.date | date_to_string }}
        &raquo; <a href="{{ post.url }}">{{ post.title }}</a>
    {% endfor %}
